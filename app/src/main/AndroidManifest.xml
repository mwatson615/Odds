<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rosebay.odds">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_odds"
        android:roundIcon="@mipmap/ic_launcher_odds_round"
        android:label="@string/app_name"
        android:name=".OddsApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-5863725671734709~8240163662"/>
        <activity android:name=".ui.mainOdds.MainActivity"
                  android:screenOrientation="portrait"
                  android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
<!--        <activity android:name=".SingleFragmentTestActivity"-->
<!--                  android:screenOrientation="portrait"-->
<!--                  android:windowSoftInputMode="adjustNothing">-->
<!--        </activity>-->
    </application>

</manifest>